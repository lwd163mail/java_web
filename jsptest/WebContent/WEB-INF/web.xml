    <servlet-name>LoadServlet</servlet-name>
    <url-pattern>/jsptest/LoadServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>LoadServletTest1</servlet-name>
    <servlet-class>test.LoadServletTest1</servlet-class>
  </servlet>
